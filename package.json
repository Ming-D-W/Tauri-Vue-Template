{
  "name": "warp-pilot",
  "version": "1.0.0",
  "description": "A modern desktop application built with Tauri 2.0, Vue 3, and Rust",
  "author": "Your Name",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run vite:dev\" \"npm run devtools\"",
    "vite:dev": "cross-env DEBUG=0 vite",
    "devtools": "vue-devtools",
    "build": "vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "lint": "eslint . --ext .vue,.js --fix",
    "format": "prettier --write \"src/**/*.{js,vue,css}\"",
    "format:check": "prettier --check \"src/**/*.{js,vue,css}\"",
    "bump": "node bump-version.cjs",
    "check": "cargo check --manifest-path src-tauri/Cargo.toml"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.9.0",
    "@vue/devtools": "^8.0.3",
    "pinia": "^3.0.3",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tauri-apps/cli": "^2.9.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.5.0",
    "prettier": "^3.6.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.1.12",
    "vite-plugin-vue-devtools": "^8.0.3"
  }
}
